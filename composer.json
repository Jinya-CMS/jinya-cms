{
  "name": "jinya/backend",
  "homepage": "https://jinya.de",
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "ext-date": "*",
    "ext-fileinfo": "*",
    "ext-imagick": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-pdo_mysql": "*",
    "ext-zip": "*",
    "jetbrains/phpstorm-attributes": "1.0",
    "jinya/plates": "4.0.0",
    "monolog/monolog": "3.3.1",
    "phpmailer/phpmailer": "v6.7.1",
    "phpmyadmin/sql-parser": "5.7.0",
    "league/climate": "3.8.2",
    "scssphp/scssphp": "v1.11.0",
    "nyholm/psr7": "1.5.1",
    "nyholm/psr7-server": "1.0.2",
    "slim/slim": "4.11.0",
    "tedivm/jshrink": "v1.4.0",
    "vlucas/phpdotenv": "v5.5.0",
    "jenssegers/agent": "v2.6.4",
    "melbahja/semver": "v1.0.1",
    "league/uri": "6.8.0",
    "jinya/pdox": "0.9.0",
    "laminas/laminas-hydrator": "4.12.0",
    "intervention/image": "2.7.2"
  },
  "suggest": {
    "ext-zend-opcache": "*",
    "ext-apcu": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "10.0.8",
    "phpstan/phpstan": "1.9.18",
    "fakerphp/faker": "v1.21.0",
    "symfony/filesystem": "v6.2.5"
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jinya\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "start": "php -S 0.0.0.0:8080 -t public",
    "test": "./vendor/bin/phpunit"
  },
  "description": "Jinya CMS is a modern content management system targeted at artists"
}
