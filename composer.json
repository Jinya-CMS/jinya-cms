{
  "name": "jinya/backend",
  "homepage": "https://jinya.de",
  "license": "MIT",
  "require": {
    "php": ">=8.0",
    "ext-fileinfo": "*",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-pdo_mysql": "*",
    "ext-zip": "*",
    "jetbrains/phpstorm-attributes": "1.0",
    "laminas/laminas-hydrator": "4.2.1",
    "guzzlehttp/guzzle": "7.3.0",
    "jinya/plates": "3.4.0",
    "monolog/monolog": "2.3.1",
    "php-di/php-di": "6.3.4",
    "phpmailer/phpmailer": "v6.5.0",
    "phpmyadmin/sql-parser": "5.4.2",
    "league/climate": "3.7.0",
    "scssphp/scssphp": "v1.6.0",
    "nyholm/psr7": "1.4.1",
    "nyholm/psr7-server": "1.0.2",
    "slim/slim": "4.8.1",
    "tedivm/jshrink": "v1.4.0",
    "vlucas/phpdotenv": "v5.3.0",
    "jenssegers/agent": "v2.6.4",
    "fakerphp/faker": "v1.15.0",
    "melbahja/semver": "1.0.1"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "squizlabs/php_codesniffer": "3.6.0"
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "scripts": {
    "start": "php -S 0.0.0.0:8080 -t public",
    "test": "./vendor/bin/phpunit",
    "phpcs": "phpcs --standard=PSR12"
  },
  "description": "Jinya CMS is a modern content management system targeted at artists"
}
