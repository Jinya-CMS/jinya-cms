{
  "name": "jinya/cms",
  "homepage": "https://jinya.de",
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "ext-date": "*",
    "ext-fileinfo": "*",
    "ext-imagick": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-pdo_mysql": "*",
    "ext-zip": "*",
    "ext-zlib": "*",
    "bacon/bacon-qr-code": "v3.0.0",
    "intervention/image": "3.7.2",
    "jenssegers/agent": "v2.6.4",
    "jetbrains/phpstorm-attributes": "1.0",
    "jinya/configuration": "0.10.0",
    "jinya/database": "0.17.0",
    "jinya/plates": "5.0.0",
    "jinya/router": "0.10.0",
    "jinya/router-extensions": "0.20.0",
    "league/climate": "3.8.2",
    "league/uri": "7.4.1",
    "laminas/laminas-hydrator": "4.15.0",
    "matomo/device-detector": "6.3.2",
    "melbahja/semver": "v1.0.1",
    "monolog/monolog": "3.7.0",
    "nyholm/psr7": "1.8.1",
    "nyholm/psr7-server": "1.1.0",
    "phpmailer/phpmailer": "v6.9.1",
    "phpmyadmin/sql-parser": "5.9.0",
    "scssphp/scssphp": "v1.12.1",
    "spomky-labs/otphp": "11.3.0",
    "symfony/filesystem": "v7.1.2",
    "tedivm/jshrink": "v1.7.0",
    "vlucas/phpdotenv": "v5.6.0"
  },
  "suggest": {
    "ext-zend-opcache": "*",
    "ext-apcu": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "fakerphp/faker": "v1.23.1",
    "laravel/pint": "v1.16.2"
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Jinya\\Cms\\": "src/",
      "Jinya\\Cms\\Migrations\\": "migrations/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jinya\\Cms\\": "tests/"
    }
  },
  "scripts": {
    "start": "php -S 0.0.0.0:8080 -t public",
    "test": "./vendor/bin/phpunit"
  },
  "description": "Jinya CMS is a modern content management system targeted at artists"
}
