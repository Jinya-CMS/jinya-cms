<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        configVersion="3"
        xmlns="https://www.phpdoc.org"
        xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/phpDocumentor/phpDocumentor/v3.3.1/data/xsd/phpdoc.xsd"
>
    <title>Jinya CMS</title>
    <paths>
        <output>docs</output>
    </paths>
    <version number="24.0.0">
        <api>
            <source dsn=".">
                <path>src</path>
            </source>
        </api>
        <guide format="rst">
            <source dsn=".">
                <path>guides</path>
            </source>
            <output>guide</output>
        </guide>
    </version>
    <setting name="guides.enabled" value="true"/>
    <template location="./phpdoc/jinya-theme"/>
</phpdocumentor>