{
  "extends": [
    "airbnb"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "globals": {
    "tinymce": "readonly"
  },
  "rules": {
    "import/extensions": [
      "error",
      {
        "js": "always",
        "json": "always"
      }
    ],
    "max-len": [
      "warn",
      120
    ],
    "no-restricted-syntax": "off",
    "indent": "off"
  }
}
